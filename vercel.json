


{
  "version": 2,
  "builds": [
    {
      "src": "api/**",
      "use": "@vercel/node"
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],

  "rewrites": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/api",
      "dest": "index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "source": "/api/user",
      "destination": "/api/user/UserController.js"
    },
    {
      "source": "/api/sticker",
      "destination": "/api/sticker/StickerController.js"
    },
    {
      "source": "/api/stickerpack",
      "destination": "/api/stickerpack/StickerpackController.js"
    },
    {
      "source": "/api/other",
      "destination": "/api/other/OtherController.js"
    }
  ],
  "env": {
