{
    "version": 2,
    "builds": [
      {
        "src": "api/**",
        "use": "@now/node"
      },
      {
        "src": "./index.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }
  